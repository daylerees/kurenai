    "keywords":     ["kurenai", "markdown", "metadata", "document", "daylerees"],
    "license":      "MIT",
    "require": {
        "erusev/parsedown-extra": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "dflydev/markdown": "1.0.*",
        "league/commonmark": "0.5.*"
    },
    "suggest": {
        "dflydev/markdown": "Parsing markdown using dflydev/markdown",
        "league/commonmark": "Parsing markdown using league/commonmark"
    },
    "autoload": {
        "psr-0": {
            "Kurenai": "src/"
        }
    }
}
